
@{
    ViewBag.Title = "ReportDuplicateView";
}

@{
    <hr />
    <div style="color:red;">
        You have already reported this campaign before, you can only report each campaign once.<br />
        You will be redirected back in 3 seconds...
    </div>
}


@section Scripts
{

    <script type="text/javascript">
        setTimeout(function ()
        {
        window.location.replace('@Url.Action("Index", "Home")')
        }, 3000);
    </script>
}

